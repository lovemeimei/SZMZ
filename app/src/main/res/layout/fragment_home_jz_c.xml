<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgrounColor">


    <RelativeLayout
        android:id="@+id/ll_SPJD"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:padding="@dimen/spacing_normal"
        android:background="@drawable/slt_list_item">

        <ImageView
            android:id="@+id/iv_spjd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/home_icon_spjd"
            android:scaleType="fitXY"/>

        <TextView
            android:id="@+id/tv_spjd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/black_light"
            android:layout_toRightOf="@+id/iv_spjd"
            android:textSize="@dimen/font_normal"
            android:layout_marginLeft="@dimen/spacing_small"
            android:text="审批进度"/>
        <TextView
            android:id="@+id/tv_spjd_detail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_spjd"
            android:layout_alignLeft="@+id/tv_spjd"
            android:layout_alignBottom="@+id/iv_spjd"
            android:textSize="@dimen/font_small"
            android:gravity="bottom"
            android:text="aaaaaa"/>

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/arrow_right"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"/>
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/gray"/>

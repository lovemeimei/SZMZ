<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">




    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/backgrounColor"
        android:orientation="vertical">
        <!--<fragment android:name="com.szmz.fragment.FragmentHomeTop"-->
            <!--android:layout_width="match_parent"-->
            <!--android:layout_height="wrap_content"/>-->
<include layout="@layout/fragment_home_top" android:visibility="gone"/>

        <RelativeLayout
            android:id="@+id/ll_dbywtz"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/spacing_normal"
            android:background="@drawable/slt_list_item"
            android:orientation="horizontal"
            android:padding="@dimen/spacing_small">

            <ImageView
                android:id="@+id/iv_dbywtz"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:scaleType="fitXY"
                android:layout_marginLeft="@dimen/spacing_normal"
                android:src="@drawable/icon_home_dbywtz" />

            <TextView
                android:id="@+id/tv_dbywtz"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/spacing_small"
                android:layout_toRightOf="@+id/iv_dbywtz"
                android:text="待办业务通知"
                android:textColor="@color/black_light"
                android:textSize="@dimen/font_big" />

            <TextView
                android:id="@+id/tv_dbywtz_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/iv_dbywtz"
                android:layout_alignLeft="@+id/tv_dbywtz"
                android:layout_below="@+id/tv_dbywtz"
                android:gravity="bottom"
                android:text="点击查看待办业务通知"
                android:textSize="@dimen/font_small" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/arrow_right"
                android:visibility="gone"
                />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:visibility="gone"
            android:background="@color/gray" />

        <RelativeLayout
            android:id="@+id/ll_spyj"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/spacing_normal"
            android:background="@drawable/slt_list_item"
            android:orientation="horizontal"
            android:padding="@dimen/spacing_small">

            <ImageView
                android:id="@+id/iv_spyj"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:scaleType="fitXY"
                android:layout_marginLeft="@dimen/spacing_normal"
                android:src="@drawable/home_02" />

            <TextView
                android:id="@+id/tv_spyj"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/spacing_small"
                android:layout_toRightOf="@+id/iv_spyj"
                android:text="审批意见"
                android:textColor="@color/black_light"
                android:textSize="@dimen/font_big" />

            <TextView
                android:id="@+id/tv_spyj_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/iv_spyj"
                android:layout_alignLeft="@+id/tv_spyj"
                android:layout_below="@+id/tv_spyj"
                android:gravity="bottom"
                android:text="点击查看审批意见"
                android:textSize="@dimen/font_small" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/arrow_right"
                android:visibility="gone"
                />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:visibility="gone"
            android:background="@color/gray" />

        <RelativeLayout
            android:id="@+id/ll_fcsx"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/slt_list_item"
            android:layout_marginTop="@dimen/spacing_normal"
            android:orientation="horizontal"
            android:padding="@dimen/spacing_small">

            <ImageView
                android:id="@+id/iv_fcsx"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:scaleType="fitXY"
                android:layout_marginLeft="@dimen/spacing_normal"
                android:src="@drawable/home_03" />

            <TextView
                android:id="@+id/tv_fcsx"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/spacing_small"
                android:layout_toRightOf="@+id/iv_fcsx"
                android:text="复查事项"
                android:textColor="@color/black_light"
                android:textSize="@dimen/font_big" />

            <TextView
                android:id="@+id/tv_fcsx_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/iv_fcsx"
                android:layout_alignLeft="@+id/tv_fcsx"
                android:layout_below="@+id/tv_fcsx"
                android:gravity="bottom"
                android:text="点击查看复查事项"
                android:textSize="@dimen/font_small" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/arrow_right"
                android:visibility="gone"
                />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/gray"
            android:visibility="gone"
            />
    </LinearLayout>

</FrameLayout>
<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="searchedit">
        <attr name="imagewidth" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="MyImageView">
        <attr name="imgSrc" format="reference"/>
        <attr name="tvContent" format="string"/>
    </declare-styleable>
    <declare-styleable name="MyImageViewV">
        <attr name="imgSrcV" format="reference"/>
        <attr name="tvContentV" format="string"/>
    </declare-styleable>

    <declare-styleable name="LoopRotarySwitchView">
        <attr name="orientation">
            <enum name="vertical" value="0" />
            <enum name="horizontal" value="1" />
        </attr>
        <attr name="autoRotation" format="boolean" />
        <attr name="r" format="dimension" />

        <attr name="direction">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>

    </declare-styleable>
    <declare-styleable name="MyLayoutView">
        <attr name="titleText" format="string" />
        <attr name="contentText" format="string" />
    </declare-styleable>
    <declare-styleable name="MyLayoutView2">
        <attr name="title" format="string" />
        <attr name="content" format="string" />
        <attr name="content2" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomLableView">
        <attr name="mLeftSrc" format="reference" />
        <attr name="mRightSrc" format="reference" />
        <attr name="mLableText" format="string" />
    </declare-styleable>

    <declare-styleable name="TabItemView">
        <attr name="icon" format="reference" />
        <attr name="color" format="color" />
        <attr name="text" format="string" />
        <attr name="text_size" format="dimension" />
    </declare-styleable>

    <declare-styleable name="MaterialRefreshLayout">
        <attr name="overlay" format="boolean" />
        <!-- style for MaterialWaveView  -->
        <attr name="wave_height_type" format="enum">
            <enum name="normal" value="0" />
            <enum name="higher" value="1" />
        </attr>
        <attr name="wave_color" format="color" />
        <attr name="wave_show" format="boolean" />
        <!-- style for CircleProgressBar  -->
        <attr name="progress_colors" format="reference" />
        <attr name="progress_stoke_width" format="dimension" />
        <attr name="progress_backgroud_color" format="color" />
        <attr name="progress_show_arrow" format="boolean" />
        <attr name="progress_arrow_width" format="dimension" />
        <attr name="progress_arrow_height" format="dimension" />
        <attr name="progress_show_circle_backgroud" format="boolean" />
        <attr name="progress_value" format="integer" />
        <attr name="progress_max_value" format="integer" />
        <attr name="progress_text_size" format="dimension" />
        <attr name="progress_text_color" format="color" />
        <attr name="progress_text_visibility" format="enum">
            <enum name="visible" value="0" />
            <enum name="invisible" value="1" />
        </attr>
        <attr name="progress_size_type" format="enum">
            <enum name="normal" value="0"></enum>
            <enum name="big" value="1"></enum>
        </attr>
        <attr name="isLoadMore" format="boolean"></attr>
    </declare-styleable>


    <declare-styleable name="HomeLab">
        <attr name="mSrc" format="reference" />
        <attr name="mNum" format="string" />
        <attr name="mTitle" format="string" />
        <attr name="mBg" format="reference" />
    </declare-styleable>

    <declare-styleable name="StepProgressView">
        <attr name="circle_radius" format="dimension" />
        <attr name="step_counts" format="integer" />
        <attr name="padding_left" format="dimension" />
        <attr name="padding_right" format="dimension" />
        <attr name="padding_top" format="dimension" />
        <attr name="padding_bottom" format="dimension" />
        <attr name="reached_color" format="color" />
        <attr name="cur_color" format="color" />
        <attr name="bg_color" format="color" />
        <attr name="stroke_width" format="dimension" />
        <attr name="auto_adapte" format="boolean" />
        <attr name="text_sizes" format="dimension" />
    </declare-styleable>

    <declare-styleable name="vericationCodeInput">

        <attr name="box" format="integer" />
        <attr name="child_h_padding" format="dimension"/>
        <attr name="child_v_padding" format="dimension"/>
        <attr name="child_width" format="dimension"/>
        <attr name="child_height" format="dimension"/>
        <attr name="padding" format="dimension"/>
        <attr name="box_bg_focus" format="reference"/>
        <attr name="box_bg_normal" format="reference"/>
        <attr name="inputType" format="string"/>
    </declare-styleable>

    <declare-styleable name="MyImageLayoutView">

        <attr name="layout_title" format="string"/>
        <attr name="layout_content" format="string"/>
        <attr name="layout_cionSrc" format="reference"/>
    </declare-styleable>
</resources>